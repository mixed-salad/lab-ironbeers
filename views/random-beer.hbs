<div id="random-beer">
    {{>beerpartial}}
        {{#each beersList}}
            <h3>Food Pairing</h3>
            <ul>
                {{#each this.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <h3>Brewers Tips:</h3>
            <p>{{this.brewers_tips}}</p>
        {{/each}}
</div>