<div id="random-beer">
    {{#each selectedBeer}}
    <img src="{{this.image_url}}" alt="">
    <h2>{{this.name}}</h2>
    <p class="tagline">{{this.tagline}}</p>
    <p class="description">{{this.description}}</p>
    <h3>Food Pairing</h3>
    <ul>
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <h3>Brewers Tips:</h3>
    <p>{{this.brewers_tips}}</p>
{{/each}}
</div>